{
    "name": "wahyu/belajar-php-unit-test",
    "description": "Belajar PHP Unit Test",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Wahyu\\Test\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wahyu\\Test\\": "tests"
        }
    },
    "authors": [
        {
            "name": "biskuatcoklat",
            "email": "wahyuandhika910@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.5"
    },
    "scripts": {
        "test": "phpunit --configuration phpunit.xml",
        "test:product": "phpunit --configuration phpunit.xml --testsuite product",
        "test:counter": "phpunit --configuration phpunit.xml --testsuite counter"
    }
}